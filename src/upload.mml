// シンバルのロテート
Int CYMINIT=0
Int TRIINIT=0
Int SNAREINIT=0
Int GONGINIT=0
Int TAIHOINIT=0

Function Snare() {
	Int CC
	IF(TIMIDITY=0) {
		CC=Snare1
	} ELSE {
		CC=38+SNAREINIT
		IF(SNAREINIT=3) {
			SNAREINIT=0
		} ELSE {
			SNAREINIT=SNAREINIT+1
		}
	}
	Result=CC
}

Function Triangle() {
	Int CC
	IF(TRIINIT=0) {
		CC=80
		TRIINIT=1
	} ELSE {
			CC=81
		TRIINIT=0
	}
	Result=CC
}



Function Cymbal() {
	Int CC
	IF(CYMINIT=0) {
		CC=56
		CYMINIT=1
	} ELSE {
		CC=57
		CYMINIT=0
	}
	Result=CC
}

Function Gong() {
	Int CC
	CC=58+GONGINIT
	IF(GONGINIT=2) {
		GONGINIT=0
	} ELSE {
		GONGINIT=GONGINIT+1
	}
	Result=CC
}

Function Taiho() {
	Int CC
	CC=29+TAIHOINIT
	IF(TAIHOINIT=3) {
		TAIHOINIT=0
	} ELSE {
		TAIHOINIT=TAIHOINIT+1
	}
	Result=CC
}

