Function Dy(Int value) {
	Int D

	D=80*128+127

	IF(TWM2=1) {
		D=100*128+110
	} ELSE {
		IF(TIMIDITY=1) {
			D=105*128+120
		}
	}
	Int Base = 127 - (D % 128)
	Int Dynamic = D / 128
	Int V
	Int V2

	V = value - Base
	V = 127 - V
	V = V * 10 * Dynamic
	V = V / 1000
	V = 127 - V
	IF(V<0) {
		V=0
	}
	IF(V>127) {
		V=127
	}
 	Result = V
}

Function VV(value) {
	Str MML="v"
	MML=MML+(ExpVol(value))
	MML
}

Function ExpVol(value) {
	Int VTempAll
	RESULT=Dy(value)
}

Function ExpExp(value) {
	RESULT=Dy(value)
}

Function RandV(Int MIN, Int MAX) {
	Str MML="v"
